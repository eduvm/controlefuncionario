<Application x:Class="Controle_de_Etiquetas.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Controle_de_Etiquetas"
             xmlns:conversores="clr-namespace:Controle_de_Etiquetas.Conversores"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Buttons.xaml"/>
                <ResourceDictionary Source="Resources/Styles/TabHeader.xaml" />
    		</ResourceDictionary.MergedDictionaries>

            <conversores:CvBarCode x:Key="CvBarCode" />
        </ResourceDictionary>
         
    </Application.Resources>
</Application>